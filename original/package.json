{
  "name": "atagspace",
  "version": "1.0.0",
  "description": "File tagging and management tool",
  "type": "module",
  "scripts": {
    "dev": "pnpm run dev:backend & pnpm run dev:frontend",
    "dev:backend": "pnpm --filter atagspace-backend dev",
    "dev:frontend": "pnpm --filter atagspace-frontend dev",
    "build": "pnpm --filter atagspace-backend build && pnpm --filter atagspace-frontend build",
    "lint": "pnpm run lint:backend && pnpm run lint:frontend",
    "lint:backend": "pnpm --filter atagspace-backend lint",
    "lint:frontend": "pnpm --filter atagspace-frontend lint",
    "typecheck": "pnpm run typecheck:backend && pnpm run typecheck:frontend",
    "typecheck:backend": "pnpm --filter atagspace-backend typecheck",
    "typecheck:frontend": "pnpm --filter atagspace-frontend typecheck"
  },
  "keywords": [
    "file",
    "tagging",
    "management"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "svelte-check": "^4.3.4"
  },
  "devDependencies": {
    "prettier": "^3.7.4",
    "prettier-plugin-svelte": "^3.4.0"
  }
}
